<div class="column-wrapper clearfix columns-{{partitionedList.columnCount}}">
	{{#eachColumn}}
		<div class="column">
			{{#each items}}
				<bh-bit bit="{.}" state="{state}"></bh-bit>
			{{/each}}
		</div>
	{{/eachColumn}}
</div>
{{#if isLoading}}
	<div class="loader">
		<i class="fa-circle-o-notch fa fa-spin"></i>
	</div>
{{/if}}
